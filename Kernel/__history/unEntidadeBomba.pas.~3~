unit uModelBomba;

interface

uses unEntidadeTanque;

type
  TBomba = class
  private
    FCodigo: Integer;
    FIdentificador: string;
    FTanque: TTanque;

    function GetCodigo: Integer;
    procedure SetCodigo(const Value: Integer);

    function GetIdentificador: string;
    procedure SetIdentificador(const Value: string);

    function GetTanque: TTanque;
    procedure SetTanque(const Value: TTanque);
  public
    property Codigo: Integer read GetCodigo write SetCodigo;
    property Descricao: string read GetDescricao write SetDescricao;
    property Tanque: TTanque read GetTanque write SetTanque;
  end;

implementation

{ TBomba }

function TBomba.GetCodigo: Integer;
begin
  Result := FCodigo;
end;

procedure TBomba.SetCodigo(const Value: Integer);
begin
  FCodigo := Value;
end;

function TBomba.GetIdentificador: string;
begin
  Result := FIdentificador;
end;

procedure TBomba.SetIdentificador(const Value: string);
begin
  FIdentificador := Value;
end;

function TBomba.GetTanque: TTanque;
begin
  Result := FTanque;
end;

procedure TBomba.SetTanque(const Value: TTanque);
begin
  FTanque := Value;
end;

end.

